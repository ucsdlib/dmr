<div class="container">
  <div class="row">
    <div class="panel panel-default">
      <table class="table table-striped table-hover table-bordered">
        <thead>
          <tr>
            <th class="col-xs-1"></th>
            <th class="col-xs-1"></th>
            <th class="col-xs-4">Title</th>
            <th class="col-xs-2">Director</th>
            <th class="col-xs-2">Call Number</th>
            <th class="col-xs-1">Year</th>
            <th class="col-xs-1">File Name</th>
          </tr>
        </thead>
        <tbody>
          <% obj.each do |m| %>
            <tr>
              <td><input type="checkbox"></td>
              <td><%=image_tag "/media/#{m.id}/#{m.file_name.gsub('.mp4','.jpg')}", :alt => '', :class => 'search-thumbnail' %></td>          
              <td><%= link_to "#{m.title}", edit_medium_path(m.id)%></td>
              <td><%= m.director %></td>
              <td><%= m.call_number %></td>
              <td><%= m.year %></td>
              <td><%= m.file_name %></td>
            </tr>
          <% end %>
        <tbody>
      </table>
    </div>
  </div>
</div>